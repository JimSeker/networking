<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/hello"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="352dp"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="16dp" />


    <Button
        android:id="@+id/btnZoomIn"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:text="Z+"
        app:layout_constraintBottom_toBottomOf="@+id/btnZoomOut"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/btnZoomOut"
        app:layout_constraintTop_toTopOf="@+id/btnZoomOut"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="32dp" />

    <Button
        android:id="@+id/btnZoomOut"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Z-"
        app:layout_constraintLeft_toRightOf="@+id/btnZoomIn"
        app:layout_constraintRight_toLeftOf="@+id/btnBack"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp"
        tools:layout_conversion_absoluteX="104dp"
        tools:layout_conversion_absoluteY="32dp" />

    <Button
        android:id="@+id/btnBack"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="@+id/btnZoomIn"
        app:layout_constraintLeft_toRightOf="@+id/btnZoomOut"
        app:layout_constraintRight_toLeftOf="@+id/btnFoward"
        app:layout_constraintTop_toTopOf="@+id/btnZoomIn"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp"
        tools:layout_conversion_absoluteX="192dp"
        tools:layout_conversion_absoluteY="32dp" />

    <Button
        android:id="@+id/btnFoward"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:text="Foward"
        app:layout_constraintBottom_toBottomOf="@+id/btnBack"
        app:layout_constraintLeft_toRightOf="@+id/btnBack"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnBack"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp"
        tools:layout_conversion_absoluteX="280dp"
        tools:layout_conversion_absoluteY="32dp" />


    <WebView
        android:id="@+id/webkit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/btnZoomIn"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnZoomIn"
        tools:layout_conversion_absoluteHeight="414dp"
        tools:layout_conversion_absoluteWidth="352dp"
        tools:layout_conversion_absoluteX="16dp"
        tools:layout_conversion_absoluteY="80dp" />

</androidx.constraintlayout.widget.ConstraintLayout>


